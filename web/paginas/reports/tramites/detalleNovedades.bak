	<parameter name="FECHA_FIN" isForPrompting="true" class="java.util.Date"/>
	<parameter name="FK_USUARIO" isForPrompting="true" class="java.lang.Long"/>
	<queryString><![CDATA[select 
t.nombre,r.fecha,f.placa,1 cantidad,
(select p.nombre from parametros p where p.fk_grupo = 3 and p.codigo = v.servicio) as servicio
from radicaciones r, formularios f , tramites t, vehiculos v
where f.fk_radicacion = r.id_radicacion
and t.id_tramite = f.fk_tramite
and v.placa = f.placa
and r.estado = 5
and r.fk_usuario = $P{FK_USUARIO}
and r.fecha between $P{FECHA_INI} and $P{FECHA_FIN}
and f.fk_tramite = $P{ID_TRAMITE}]]></queryString>

