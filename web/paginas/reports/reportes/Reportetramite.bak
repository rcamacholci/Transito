	<queryString><![CDATA[select t.id_tramite,t.nombre, SUM(s.valor) valor
from tramite_conceptos tc, sede_tramite_conceptos s, tramites t
where s.fk_concepto = tc.fk_concepto
and  tc.fk_tramite in (t.id_tramite)
and tc.fk_tramite = s.fk_tramite
And t.clase_tramite  = $P{TRAMITE}
and s.fecha_fin is null
and (tc.valor_1=$P{CLASE} or tc.valor_1 is null) group by t.nombre, t.id_tramite order by t.id_tramite]]></queryString>

