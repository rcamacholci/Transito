	<parameter name="FECHA_FIN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="fk_usuario" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT RP.FECHA,RP.PLACA,RP.MOTOR,RP.CHASIS,
(SELECT PAR.NOMBRE_CORTO ||' - '||PER.DOCUMENTO FROM PERSONAS PER, PARAMETROS PAR WHERE PER.ID_PERSONA = RP.FK_PERSONA AND PER.TIPO_DOCUMENTO = PAR.CODIGO AND PAR.FK_GRUPO = 5) SOLICITANTE,
(SELECT U.NOMBRE FROM USUARIOS U WHERE U.ID_USUARIO = RP.FK_USUARIO) USUARIO
FROM RUNT_PREASIGNACION RP
WHERE RP.ESTADO = 1
AND TO_DATE(TO_CHAR(RP.FECHA,'dd/MM/YYYY'),'dd/MM/yyyy') between $P{FECHA_INI} and $P{FECHA_FIN}
AND (FK_USUARIO = $P{fk_usuario} or $P{fk_usuario} = 0) 
ORDER BY RP.FECHA]]></queryString>

