	<parameter name="FECHA_FIN" isForPrompting="true" class="java.lang.String"/>
	<parameter name="ID_ENTIDAD" isForPrompting="true" class="java.lang.Long"/>
	<parameter name="USUARIO" isForPrompting="true" class="java.lang.Long"/>
	<parameter name="CAJA" isForPrompting="true" class="java.lang.Long"/>
	<queryString><![CDATA[SELECT NVL(DECODE(C.ID_CONCEPTO,4,10,324,323),C.ID_CONCEPTO) ID_CONCEPTO,
NVL(DECODE(C.ID_CONCEPTO,4,'TRANSFORMACION',323,'RECATEGORIZACION',324,'RECATEGORIZACION'),C.NOMBRE) NOMBRE,
COUNT(C.ID_CONCEPTO) CANTIDAD, SUM(D.VALOR) VALOR
FROM PAGOS P,DISTRIBUCION D,CONCEPTOS C
WHERE P.ID_PAGO = D.FK_PAGO
AND P.ESTADO = 3
AND C.ID_CONCEPTO = D.FK_CONCEPTO_DISTRIBUCION
AND D.FK_ENTIDAD_DISTRIBUCION = $P{ID_ENTIDAD}
and P.FK_TAQUILLA IN(SELECT ID_TAQUILLA FROM TAQUILLAS WHERE (FK_USUARIO = $P{USUARIO} OR $P{USUARIO} = 0) AND (FK_CAJA = $P{CAJA} OR $P{CAJA} = 0) 
AND TO_DATE(TO_CHAR(FECHA_INI,'dd/MM/yyyy'),'dd/MM/yyyy') BETWEEN TO_DATE($P{FECHA_INI},'dd/MM/yyyy') AND TO_DATE($P{FECHA_FIN},'dd/MM/yyyy') AND ESTADO = 5)
GROUP BY NVL(DECODE(C.ID_CONCEPTO,4,10,324,323),C.ID_CONCEPTO),
NVL(DECODE(C.ID_CONCEPTO,4,'TRANSFORMACION',323,'RECATEGORIZACION',324,'RECATEGORIZACION'),C.NOMBRE)
ORDER BY 2]]></queryString>

