	<parameter name="MODELO_INI" isForPrompting="true" class="java.lang.String"/>
	<parameter name="MODELO_FIN" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select 
v.modelo,
v.placa,
(select m.nombre ||'  -  '|| l.nombre from lineas l, marcas m where m.codigo = l.fk_marca and v.fk_linea = l.id_linea) MARCA,
(select p.nombre from parametros p where p.fk_grupo = 3 and p.codigo = v.servicio) SERVICIO,
NVL((select sum(SALDO) from carteras ca where ca.fk_vehiculo = v.id_vehiculo),0) CARTERA,
NVL((select MIN(car.referencia) ||'  -  '|| max(car.referencia) from carteras car where car.fk_vehiculo = v.id_vehiculo),0) VIGENCIA
from vehiculos v where 
v.modelo BETWEEN $P{MODELO_INI} and $P{MODELO_FIN}
order by v.modelo]]></queryString>

