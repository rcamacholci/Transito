	<parameter name="FECHA_INI" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA_FIN" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select u.nombre as usuario, v.placa,pc.fecha_prestamo, c.folios, pe.nombre as estado, pm.nombre as motivo
from prestamos_carpetas pc, carpetas c, vehiculos v, usuarios u, parametros pe, parametros pm
where pc.fk_usuario = u.id_usuario
and pc.fk_carpeta = c.id_carpeta
and c.fk_vehiculo = v.id_vehiculo
and pm.fk_grupo = 334
and pm.codigo = pc.motivo
and pe.fk_grupo = 337
and pe.codigo = pc.estado
and pc.estado in (2,3)
and TO_CHAR(pc.fecha_prestamo,'dd/MM/yyyy') between $P{FECHA_INI} and $P{FECHA_FIN}
order by pc.fecha_prestamo]]></queryString>

