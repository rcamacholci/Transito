FROM FORMULARIOS F, TRAMITES T , RADICACIONES R, VEHICULOS V
WHERE F.FK_TRAMITE=T.ID_TRAMITE
AND R.ID_RADICACION = F.FK_RADICACION
AND R.ESTADO IN(4,5)
AND V.PLACA = F.PLACA
AND V.ID_VEHICULO = $P{ID_VEHICULO}
AND (T.ID_TRAMITE < 33 OR T.ID_TRAMITE = 58)
UNION
SELECT FECHA,NOMBRE_TRAMITE||'  -  FUNAL/OBS.: '||OBSERVACION FROM TRAMITES_ASOCIADOS
WHERE FK_VEHICULO = $P{ID_VEHICULO} AND ESTADO = 'APROBADA ITA'
