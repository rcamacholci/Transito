	<parameter name="FECHA_FIN" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT N.NUMERO,N.FECHA,
(SELECT PLACA FROM VEHICULOS V WHERE FK_VEHICULO = ID_VEHICULO) VEHICULO,
(SELECT M.NOMBRE_CORTO||' '||P.DOCUMENTO||' - '||P.NOMBRE_1||' '||NVL(P.NOMBRE_2,'')||P.APELLIDO_1||' '||NVL(P.APELLIDO_2,'') FROM PERSONAS P,PARAMETROS M WHERE P.ID_PERSONA = N.FK_PERSONA AND M.CODIGO = P.TIPO_DOCUMENTO AND M.FK_GRUPO = 5) SOLICITANTE,
N.VALOR,N.REFERENCIA,
(SELECT M.NOMBRE FROM PARAMETROS M WHERE M.CODIGO = N.ESTADO AND M.FK_GRUPO = 350) ESTADO,
N.FECHA_PROCESO,
(SELECT U.NOMBRE FROM USUARIOS U WHERE U.ID_USUARIO = N.FK_USUARIO) USUARIO
FROM NOTAS_CREDITO N
WHERE TO_DATE(TO_CHAR(N.FECHA_PROCESO,'dd/MM/YYYY'),'dd/MM/yyyy') between $P{FECHA_INI} and $P{FECHA_FIN}
ORDER BY N.ID_NOTA_CREDITO]]></queryString>

