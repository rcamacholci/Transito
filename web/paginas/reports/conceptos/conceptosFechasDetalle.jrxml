	<parameter name="ID_CONCEPTO" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA_INI" isForPrompting="true" class="java.lang.String"/>
	<parameter name="FECHA_FIN" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[Select Tmp.Fecha,Tmp.Persona,Tmp.Placa,Tmp.Numero_Tiquete,Tmp.Nombre,Tmp.Valor From
(select ti.fecha,
(select pm.nombre_corto||' '||pe.documento||' - '||pe.nombre_1||' '||pe.apellido_1 from 
personas pe,parametros pm where pe.tipo_documento = pm.codigo and pm.fk_grupo = 5
and pe.id_persona = ti.fk_persona) PERSONA,
(NVL((select v.placa from vehiculos v where v.id_vehiculo = ti.fk_vehiculo),NVL(ti.descripcion,'Financiacion'))) as placa,
ti.numero_tiquete, co.nombre, dc.valor Valor
from tiquetes ti, detalle_tiquetes dt, detalle_carteras dc, pagos pa, conceptos co,taquillas q
where ti.id_tiquete = dt.fk_tiquete
and ti.id_tiquete = pa.fk_tiquete
and pa.id_pago = dc.fk_pago
and dc.fk_cartera = dt.fk_cartera
and ti.estado = 3
and ti.fk_taquilla = q.id_taquilla
and q.estado = 5
and dc.fk_concepto = $P{ID_CONCEPTO}
and co.id_concepto = dc.fk_concepto
and to_date(ti.fecha) between to_date($P{FECHA_INI},'dd/MM/yyyy') and to_date($P{FECHA_FIN},'dd/MM/yyyy')
group by Dc.Referencia,Dc.Fk_Cartera,ti.fecha,ti.fk_persona,ti.descripcion,ti.fk_vehiculo,ti.numero_tiquete, co.nombre,Dc.valor,Dc.Id_Detalle_Cartera
Union All
select ti.fecha,
(select pm.nombre_corto||' '||pe.documento||' - '||pe.nombre_1||' '||pe.apellido_1 from 
personas pe,parametros pm where pe.tipo_documento = pm.codigo and pm.fk_grupo = 5
and pe.id_persona = ti.fk_persona) PERSONA,
(NVL((select v.placa from vehiculos v where v.id_vehiculo = ti.fk_vehiculo),NVL(ti.descripcion, 'N/A'))) as placa,
ti.numero_tiquete,co.nombre ,sum(tc.valor) valor
from tiquetes ti, detalle_tiquetes dt, tiquete_conceptos tc, conceptos co,taquillas q
where ti.id_tiquete = dt.fk_tiquete
and dt.id_det_tiquete = tc.fk_detalle_tiquete
and ti.estado = 3
and ti.fk_taquilla = q.id_taquilla
and q.estado = 5
and to_date(ti.fecha) between to_date($P{FECHA_INI},'dd/MM/yyyy') and to_date($P{FECHA_FIN},'dd/MM/yyyy')
and tc.fk_concepto = $P{ID_CONCEPTO}
and co.id_concepto = tc.fk_concepto
group by ti.fecha,ti.fk_persona,ti.descripcion,ti.fk_vehiculo,ti.numero_tiquete, co.nombre)
Tmp
Order by Tmp.Fecha Asc]]></queryString>

